{
  "expo": {
    "name": "ClimaCast",
    "slug": "climacast",
    "version": "1.2.2",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/app/icon.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF",
      "image": "./assets/images/app/splash.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.bytewise.climacast"
    },
    "android": {
      "versionCode": 7,
      "userInterfaceStyle": "automatic",
      "package": "com.bytewise.climacast",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/app/adaptive-icon.png"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics"
    ],
    "extra": {
      "eas": {
        "projectId": "f212fbbb-e6c0-4ae2-8ad9-33577f169857"
      }
    }
  }
}
